<h1> All Users </h1>
<table class = "table table-striped">
	<thead>
		<tr>
			<td class="col-sm-3">Name</td>
			<td class="col-sm-3">Email</td>
		</tr>
	</thead>
<% @users.each do |user| %>
<tr>

	<td> <%= link_to "#{user.name}", user_path(user[:id])%> </td>
	<td> <%= user.email %> </td>
</tr>
<% end %>
</table>